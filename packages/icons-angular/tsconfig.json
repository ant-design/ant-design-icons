{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": false,
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "lib": [
      "esnext",
      "dom"
    ],
    "paths": {
      "@ant-design/icons-angular": [
        "src/public_api"
      ],
      "@ant-design/icons-angular/icons": [
        "src/icons/public_api"
      ]
    },
    "useDefineForClassFields": false
  }
}
