{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "esModuleInterop": true,
    "target": "es2020",
    "types": [
      "jasmine",
      "node"
    ],
    "paths": {
